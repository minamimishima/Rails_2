<%= form_with(model: @user, url: profile_edit_path, local: true, method: :post) do |f| %>
<table>
  <tr>
    <th><%= f.label :name,"名前" %></th>
    <td><%= f.text_field :name %></td>
  </tr>

  <tr>
    <th><%= f.label :image, "画像" %></th>
    <td><%= f.file_field :image %></td>
  </tr>

  <tr>
    <th><%= f.label :introduction, "自己紹介" %></th>
    <td><%= f.text_field :introduction %></td>
  </tr>

</table>
<%= f.submit "編集" %>

<% end %>
